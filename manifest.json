{
  "name": "Pair Programming Timer",
  "description": "Timer for Pair Programming",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
    "128": "assets/icons/128.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications",
    "storage"
  ],
  "action": {
    "default_popup": "public/index.html"
  },
  "options_page": "public/index.html"
}